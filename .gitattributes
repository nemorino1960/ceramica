# Auto detect text files and perform LF normalization
* text=auto

# Documents
*.md text
*.txt text
*.json text

# Python
*.py text
*.pyx text
*.pxd text
*.pxi text

# Templates
*.html text
*.css text
*.js text
*.xml text

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.woff2 binary
*.pyc binary
*.pdf binary

# Archives
*.7z binary
*.jar binary
*.rar binary
*.zip binary
*.gz binary
*.tgz binary
*.bzip2 binary

# Exclude files from export
.gitattributes export-ignore
.gitignore export-ignore
.github export-ignore
